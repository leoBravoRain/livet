erStore\n                        goToInstagram={() => {\n                            // alert(\"Go to home\")\n                            window.open(this.state.store.instagramUrl);\n                        }}\n\n                        // display whatsapp\n                        displayToWhatsapp={this.state.store.whatsappNumber != null}\n                        goToWhatsapp={() => {\n                            window.open(\"https://wa.me/\" + this.state.store.whatsappNumber);\n                        }}\n\n                        // display facebook\n                        displayToFacebook={this.state.store.facebookUrl != null}\n                        goToFacebook={() => {\n                            window.open(this.state.store.facebookUrl);\n                        }}\n                    />\n                    \n                </Container>\n\n                :\n\n                <CircularProgress />\n\n        );\n\n    }\n\n}\n\nexport default SaleConfirmation;","/home/leo/Desktop/livet/webapp/web-platform/src/libraries/firebase/firebase.js",[],"/home/leo/Desktop/livet/webapp/web-platform/src/screens/owner/register/register.screen.js",[],"/home/leo/Desktop/livet/webapp/web-platform/src/screens/owner/productsToSell/productsToSell.screen.js",[],"/home/leo/Desktop/livet/webapp/web-platform/src/screens/owner/generalComponents/menuBar.component.js",["168"],"import React from \"react\";\n\n// import { BrowserRouter as Router, Route, Link } from \"react-router-dom\";\n\n// material ui\n// import Paper from '@material-ui/core/Paper';\nimport Container from '@material-ui/core/Container';\n// import TextField from '@material-ui/core/TextField';\nimport { Button } from \"@material-ui/core\";\n// import FormControl from '@material-ui/core/FormControl';\n// import Typography from '@material-ui/core/Typography';\n// import CircularProgress from \"@material-ui/core/CircularProgress\";\n// import Grid from '@material-ui/core/Grid';\n\nimport Add from \"@material-ui/i